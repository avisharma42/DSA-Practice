class Solution {
public:
    int minimizedMaximum(int n, vector<int>& quantities) {
        int low = 1;
        int high = *max_element(quantities.begin() , quantities.end());
        while(low<high){
            int mid = low+(high-low)/2;
            long long shop = 0;
            for(int q : quantities){
                shop += (q+mid-1)/mid;
            }
            if(shop<=n) high = mid;
            else low = mid+1;
        }
        return low;
    }
};
